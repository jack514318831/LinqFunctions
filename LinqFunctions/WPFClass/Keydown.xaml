<Window x:Class="LinqFunctions.WPFClass.Keydown"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LinqFunctions.WPFClass"
        mc:Ignorable="d"
        Title="Keydown" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Name="tb_input" Margin="10" KeyDown="tb_input_KeyDown"/>
            <TextBox Name="tb_output" Margin="10" Height="200" />
        </StackPanel>
    </Grid>
</Window>
